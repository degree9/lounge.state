(ns lounge.state
  (:require [lounge.state.route :as route]
            [lounge.state.auth :as auth])
  (:require-macros [javelin.core :refer [defc defc= cell=]]))

(def user    (cell= {:auth   auth/authenticated?
                     :unauth auth/unauthenticated?}))
(def routing (cell= {:hash   route/route-hash}))

(cell= (println user routing))
